{
  "name": "ilib",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/compiler-cli": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@types/node": "^6.0.55",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "node-static": "^0.7.9",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rxjs": "^5.0.2",
    "style-loader": "^0.13.1",
    "systemjs": "0.19.41",
    "typescript": "~2.0.3",
    "webpack": "^1.14.0",
    "zone.js": "^0.7.4"
  },
  "scripts": {
    "build:core": "tsc -p core/src",
    "build:ng:src:generator:compile": "tsc -p angular/generator/src",
    "build:ng:src:generator:run": "node angular/generator/dist/index.js",
    "build:ng:src:generator": "npm run build:ng:src:generator:compile -s && npm run build:ng:src:generator:run -s",
    "build:ng:src:compile": "ngc -p angular/src",
    "build:ng:src": "npm run build:ng:src:generator -s && npm run build:ng:src:compile -s",
    "build:ng:demo": "ngc -p angular/demo/src",
    "build:ng": "npm run build:ng:src -s && npm run build:ng:demo -s",
    "build:react:src:generator:compile": "tsc -p react/generator/src",
    "build:react:src:generator:run": "node react/generator/dist/index.js",
    "build:react:src:generator": "npm run build:react:src:generator:compile -s && npm run build:react:src:generator:run -s",
    "build:react:src:compile": "babel react/src -d react/dist -q",
    "build:react:src": "npm run build:react:src:generator -s && npm run build:react:src:compile -s",
    "build:react:demo": "webpack --config react/demo/webpack.config.js",
    "build:react": "npm run build:react:src -s && npm run build:react:demo -s",
    "build": "npm run build:core -s && npm run build:ng -s && npm run build:react -s",
    "serve": "static -p 3000 -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}'"
  },
